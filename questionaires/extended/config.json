{
  "name": "extended",
  "groups": {
    "symptoms": {
      "fever_start": {
        "type": "date"
      },
      "fever_suddenly": {
        "type": "boolean"
      },
      "cough_severity": {
        "type": "likert"
      },
      "fatigue_severity": {
        "type": "likert"
      },
      "cough_slime_severity": {
        "type": "likert"
      },
      "short_breath_severity": {
        "type": "likert"
      },
      "muscle_ache_severity": {
        "type": "likert"
      },
      "sore_throat_severity": {
        "type": "likert"
      },
      "headache_severity": {
        "type": "likert"
      },
      "chills_severity": {
        "type": "likert"
      },
      "nauseous_severity": {
        "type": "likert"
      }
    },
    "social": {
      "social_contacts": {
        "type": "multiselect",
        "options": [
          "teenagers",
          "elderly",
          "patients",
          "groups"
        ]
      },
      "household_babies": {
        "type": "integer"
      },
      "household_children": {
        "type": "integer"
      },
      "household_adults": {
        "type": "integer"
      },
      "household_seniors": {
        "type": "integer"
      },
      "household_elderly": {
        "type": "integer"
      },
      "household_children_external_contact": {
        "type": "integer",
        "conditionals": [
          {
            "question": "household_children",
            "not_option": 0
          }
        ]      
      }
    },
    "transport": {
      "transport_means": {
        "type": "select",
        "other": true,
        "options": [
          "walking",
          "bicycle",
          "bike",
          "car",
          "public"
        ]
      },
      "transport_time": {
        "type": "integer",
        "variant": "minutes",
        "min": 0,
        "max": 240
      }
    },
    "medical": {
      "medication": {
        "type": "multiselect",
        "options": [
          "asthma",
          "diabetes",
          "lung_disorder",
          "heart_disorder",
          "kidney_disorder",
          "immunocompromising_condition"
        ]
      },
      "tobacco": {
        "type": "integer",
        "required": false,
        "min": 0,
        "max": 20
      },
      "allergies": {
        "type": "multiselect",
        "options": [
          "hay_fever",
          "dust_mite",
          "animals",
          "other_respiratory_allergies"
        ]
      },
      "travel_before_symptoms": {
        "type": "boolean"
      },
      "travel_before_symptoms_countries": {
        "type": "multiselect",
        "variant": "country",
        "conditionals": [
          {
            "question": "travel_after_symptoms",
            "option": true
          }
        ]
      }
    }
  }
}
